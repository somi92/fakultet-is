@{
    ViewBag.Title = "Pocetna | Fakultet-IS";
    ViewBag.Message = "Informacioni sistem za pracenje rada fakulteta";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title </h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
